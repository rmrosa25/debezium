{
  "name": "oracle-connector",
  "config": {
    "connector.class": "io.debezium.connector.oracle.OracleConnector",
    "tasks.max": "1",
    "database.hostname": "oracle",
    "database.port": "1521",
    "database.user": "c##dbzuser",
    "database.password": "dbz",
    "database.dbname": "ORCLCDB",
    "database.pdb.name": "ORCLPDB1",
    "database.server.name": "oracle-server",
    "schema.history.internal.kafka.topic": "schema-changes.oracle",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:29092",
    "schema.history.internal.kafka.topic.replication.factor": "3",
    "table.include.list": "DEBEZIUM.CUSTOMERS,DEBEZIUM.PRODUCTS,DEBEZIUM.INVOICES",
    "schema.include.list": "DEBEZIUM",
    "database.connection.adapter": "logminer", 
    "database.out.server.name": "DBZXOUT",
    "database.out.queue.name": "Q$_DBZXOUT_2",
    "xstream.username": "c##dbzuser",
    "xstream.password": "dbz",
    "xstream.server.name": "DBZXOUT",
    "xstream.queue.name": "Q$_DBZXOUT_2",
    "log.mining.strategy": "redo_log_catalog",
    "log.mining.continuous.mine": "false",
    "snapshot.mode": "when_needed",
    "snapshot.fetch.size": "2000",
    "snapshot.delay.ms": "0",
    "decimal.handling.mode": "double",
    "time.precision.mode": "adaptive",
    "tombstones.on.delete": "true",
    "provide.transaction.metadata": "true",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.unwrap.add.fields": "op,source.ts_ms",
    "topic.prefix": "oracle-server",
    "poll.interval.ms": "1000"
  }
}